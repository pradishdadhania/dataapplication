<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dataapplication">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DataApplication">
        <activity android:name=".SharedPreferencesDemo"></activity>
        <activity android:name=".ModifyCountryActivity" />
        <activity android:name=".AddCountryActivity" />
        <activity android:name=".CountryListActivity" />
        <activity android:name=".sqllight" />
        <activity android:name=".ViewInformationActivity" />
        <activity android:name=".ExternalStorage" />
        <activity android:name=".InternalStorageAndroid" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>